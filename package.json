{
  "name": "cok-people-search-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node local.js",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage production",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "build:browser:prod": "ng build --prod --configuration=production --base-href /",
    "build:server:prod": "webpack --config webpack.server.config.js --progress --colors  --mode production",
    "build:prod:deploy": "npm run build:prod && npm run deploy:prod",
    "build:dev": "npm run build:browser:dev && npm run build:server:dev",
    "build:browser:dev": "ng build --configuration=dev --base-href /",
    "build:server:dev": "webpack --config webpack.server.config.js --progress --colors --mode development",
    "build:dev:deploy": "npm run build:dev && npm run deploy:dev",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb-v2-node": "0.1.0-preview.2",
    "auth0": "^2.18.0",
    "auth0-js": "^9.11.2",
    "aws-param-store": "^3.0.0",
    "aws-serverless-express": "^3.3.6",
    "aws-ssm-params": "^1.0.2",
    "classlist.js": "^1.1.20150312",
    "console-polyfill": "^0.3.0",
    "core-js": "^2.6.9",
    "cors": "~2.8.4",
    "cp-cli": "^2.0.0",
    "express-jwt": "^5.3.1",
    "hashmap": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "mixpanel": "^0.10.2",
    "rxjs": "~6.3.3",
    "sha-256-js": "^1.0.3",
    "snyk": "^1.226.2",
    "then-request": "^6.0.2",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "opencollective": "^1.0.3",
    "protractor": "~5.4.0",
    "serverless": "^1.50.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-apigwy-binary": "^1.0.0",
    "serverless-associate-waf": "^1.1.0",
    "serverless-domain-manager": "^3.2.6",
    "serverless-plugin-include-dependencies": "^3.2.1",
    "ts-loader": "4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.8"
  },
  "snyk": true
}
